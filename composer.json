{
  "name": "sorokinmedia/yii2-helpers",
  "description": "Helpers for Yii2",
  "type": "yii2-extension",
  "keywords": ["yii2", "yii 2", "helper", "plural", "date"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Ruslan Gilyazetdinov",
      "email": "ma3obblu@gmail.com",
      "homepage": "https://www.facebook.com/Ma3oBblu"
    }
  ],
  "support": {
    "issues": "https://github.com/sorokinmedia/yii2-helpers/issues?state=open",
    "source": "https://github.com/sorokinmedia/yii2-helpers"
  },
  "require": {
    "yiisoft/yii2": ">=2.0.30",
    "ext-intl": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "8.*"
  },
  "autoload": {
    "psr-4": {
      "sorokinmedia\\helpers\\": "src/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
